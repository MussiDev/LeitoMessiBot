const Distube = require("distube");
  client.distube = new Distube(client, {
    emitNewSongonly: true,
    searchSongs: false,
    leaveOnStop: true,
    leaveOnFinisg: true,
    leaveOnEmpty: true,
  });
  const status = (queue) =>
    `Volumen: \`${queue.volume}%\` | Loop: \`${
      queue.repeatMode
        ? queue.repeatMode == 2
          ? "Server Queue"
          : "Esta canción"
        : "Apagado"
    }\` | Autoplay: \`${queue.autoplay ? "On" : "Off"}\``;
  client.distube.on("addList", (message, queue, playlist) =>
    message.channel.send(
      `Playlist agregada \`${playlist.name}\` (${
        playlist.songs.length
      } canciones)\n${status(queue)}`
    )
  );
  client.distube.on("addSong", (message, song) =>
    message.channel.send(
      `Canción agregada ${song.name} - \`${song.formattedDuration}\` por ${song.user}`
    )
  );
  client.distube.on("playSong", (message, song) => {
    message.channel.send(
      `Reproduciendo ${song.name} - \`${song.formattedDuration}`
    );
  });
  client.distube.on("playList", (message, playlist) => {
    message.channel.send(
      `Reproduciendo playlist ${playlist.name} - \`${playlist.formattedDuration}`
    );
  });
  client.distube.on("error", (error) => {
    console.log(error);
  });